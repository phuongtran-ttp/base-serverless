    "serverless-webpack": "^5.3.1",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.2",
