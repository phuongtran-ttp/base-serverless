    "@types/node": "^12.0.12",
    "@types/pino": "^7.0.5",
    "@types/validator": "^10.11.2",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "pino-pretty": "^3.2.0",
    "prettier": "^1.18.2",
    "serverless": "^3.10.2",
