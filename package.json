{
  "name": "serverless-graphql-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "up": "yarn down && docker-compose up -d",
    "down": "docker-compose down",
    "sls:offline": "sls offline",
    "sls:deploy": "cross-env NODE_ENV=production sls deploy",
    "sls:deploy:prod": "cross-env NODE_ENV=production sls deploy --stage prod --region ap-northeast-2",
    "sls:deploy:api": "yarn sls:deploy -f api",
    "sls:logs": "sls logs -t",
    "sls:logs:api": "yarn sls:logs -f api",
    "sls:logs:playground": "yarn sls:logs -f playground",
    "start": "yarn sls:offline --noEnvironment | pino-pretty",
    "docker:start": "yarn force-sync && yarn start",
    "fake:generate": "cross-env NODE_ENV=development ts-node ./fakers/generateData"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.27",
